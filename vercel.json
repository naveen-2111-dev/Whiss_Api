{
    "version": 2,
    "builds": [
        {
            "src": "dist/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}